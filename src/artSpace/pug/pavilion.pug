        div(class='pavilion_main_btn center')
            div(class='pavilion_main_btns box_h center_s')
                div(class='pavilion_btns_left')
                div(class='pavilion_btns_text boxflex1')
                    div AURA SPACE
                    p GALLERY
                div(class='pavilion_btns_right')

